{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy Source to raw",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"deleteFilesAfterCompletion": false
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "Binary",
						"type": "DatasetReference",
						"parameters": {
							"container": "simulatingasource",
							"directory": "directory",
							"file": "SampleData.xlsx"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "Binary",
						"type": "DatasetReference",
						"parameters": {
							"container": "raw",
							"directory": "sales",
							"file": "SampleData.xlsx"
						}
					}
				]
			},
			{
				"name": "Create parquet",
				"type": "DatabricksNotebook",
				"dependsOn": [
					{
						"activity": "Copy Source to raw",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": "/Data factory demo/Data factory Notebook"
				},
				"linkedServiceName": {
					"referenceName": "AzureDatabricks1",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "Copy parquet to csv",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Create parquet",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ParquetSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFolderPath": "sales/SampleData.parquet",
							"wildcardFileName": "*.parquet",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "ParquetReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "MergeFiles"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Parquet",
						"type": "DatasetReference",
						"parameters": {
							"container": "raw",
							"directory": "sales/SampleData.parquet",
							"file": "."
						}
					}
				],
				"outputs": [
					{
						"referenceName": "Csv",
						"type": "DatasetReference",
						"parameters": {
							"container": "staging",
							"directory": "sales",
							"file": "SampleData.csv"
						}
					}
				]
			}
		],
		"annotations": []
	}
}